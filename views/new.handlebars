<div id="containerCreate">
  <h2>新增餐廳資料</h2>
  <form action="/restaurants" method="POST">
    <div>
      <label for="name">餐廳名字 ： </label>
      <input type="text" id="name" name="name">
    </div>
    <div>
      <label for="name_en">餐廳英文 ： </label>
      <input type="text" id="name_en" name="name_en">
    </div>
    <div>
      <label for="category">分　類 ： </label>
      <input type="text" id="category" name="category">
    </div>
    <div>
      <label for="image">圖片位址 ： </label>
      <input type="text" id="image" name="image">
    </div>
    <div>
      <label for="location">餐廳地址 ： </label>
      <input type="text" id="location" name="location">
    </div>
    <div>
      <label for="phone">餐廳電話 ： </label>
      <input type="text" id="phone" name="phone">
    </div>
    <div>
      <label for="google_map">google map ： </label>
      <input type="text" id="google_map" name="google_map">
    </div>
    <div>
      <label for="rating">餐廳評分 ： </label>
      <input type="text" id="rating" name="rating">
    </div>
    <div>
      <label for="description">餐廳介紹 ： </label>
      <input type="text" id="description" name="description">
    </div>
    <button type="submit">Create</button>
  </form>
</div>




